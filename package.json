    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "source-map-support": "^0.5.16",
    "toastr": "^2",
    "typeface-roboto": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "bufferutil": "^4.0",
    "devtron": "^1.4",
    "electron": "^11.5.0",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2",
    "electron-webpack": "^2.6",
    "electron-window-state": "^5.0",
